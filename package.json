{
  "author": "Emmanuel Payet",
  "name": "simple-restful",
  "description": "Easily create RESTful API",
  "version": "2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/epayet/SimpleRestJS.git"
  },
  "main": "./lib/index.js",
  "keywords": [
    "rest",
    "server",
    "restful",
    "simple",
    "api"
  ],
  "dependencies": {
    "fs-promise": "^0.5.0",
    "restify": "^4.0.4",
    "winston": "^2.2.0"
  },
  "engines": {
    "node": "*"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-core/register test/**/**",
    "test:unit": "mocha --compilers js:babel-core/register test/unit/**/**",
    "test:integration": "mocha --compilers js:babel-core/register test/integration/**/**",
    "test:watch": "npm test -- --watch",
    "prepublish": "babel ./src --out-dir ./lib"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0"
  }
}
